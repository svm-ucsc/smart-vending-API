{
    "name": "smv-fastify",
    "version": "1.0.0",
    "description": "The node server for the SMV API",
    "main": "app.js",
    "scripts": {
        "pretest": "standard",
        "lint": "standard --fix",
        "start": "fastify start -a 0.0.0.0 -l info app.js",
        "dev": "fastify start -w -l info -P app.js",
        "test": "tap \"test/**/*.test.js\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.51.0",
        "@aws-sdk/lib-dynamodb": "^3.52.0",
        "@aws-sdk/util-dynamodb": "^3.52.0",
        "fastify": "^3.27.2",
        "fastify-autoload": "^3.10.0",
        "fastify-cli": "^2.15.0",
        "fastify-cors": "^6.0.3",
        "fastify-env": "^2.1.1",
        "fastify-mqtt": "^1.0.0",
        "fastify-plugin": "^3.0.0",
        "fastify-sensible": "^3.1.2",
        "fastify-swagger": "^4.17.0"
    },
    "devDependencies": {
        "pino-pretty": "^7.5.1",
        "standard": "^11.0.1",
        "tap": "^15.1.6"
    },
    "tap": {
        "check-coverage": false
    }
